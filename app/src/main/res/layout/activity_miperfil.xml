<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".miperfil">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginStart="46dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="10dp"
        android:gravity="center"
        android:text="Mi Perfil"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/etnombres2"
        app:layout_constraintStart_toStartOf="@+id/etnombres2"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/etapellidos2"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginBottom="16dp"
        android:ems="10"
        android:hint="Apellidos"
        android:inputType="textPersonName"
        android:paddingLeft="20dp"
        app:layout_constraintBottom_toTopOf="@+id/etcedula2"
        app:layout_constraintStart_toStartOf="@+id/etnombres2"
        app:layout_constraintTop_toBottomOf="@+id/etnombres2" />

    <EditText
        android:id="@+id/etcedula2"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginBottom="11dp"
        android:ems="10"
        android:hint="Cedula"
        android:inputType="number"
        android:paddingLeft="20dp"
        app:layout_constraintBottom_toTopOf="@+id/ettelefono2"
        app:layout_constraintStart_toStartOf="@+id/etapellidos2"
        app:layout_constraintTop_toBottomOf="@+id/etapellidos2" />

    <EditText
        android:id="@+id/ettelefono2"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginBottom="13dp"
        android:ems="10"
        android:hint="Telefono o celular"
        android:inputType="number"
        android:paddingLeft="20dp"
        app:layout_constraintBottom_toTopOf="@+id/etRegEmail2"
        app:layout_constraintStart_toStartOf="@+id/etcedula2"
        app:layout_constraintTop_toBottomOf="@+id/etcedula2" />

    <EditText
        android:id="@+id/etRegEmail2"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginBottom="19dp"
        android:ems="10"
        android:hint="Email o Correo"
        android:inputType="textPersonName"
        android:paddingLeft="20dp"
        app:layout_constraintBottom_toTopOf="@+id/etRegUsuario2"
        app:layout_constraintStart_toStartOf="@+id/ettelefono2"
        app:layout_constraintTop_toBottomOf="@+id/ettelefono2" />

    <EditText
        android:id="@+id/etnombres2"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginBottom="15dp"
        android:ems="10"
        android:hint="Nombres"
        android:inputType="textPersonName"
        android:paddingLeft="20dp"
        app:layout_constraintBottom_toTopOf="@+id/etapellidos2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <EditText
        android:id="@+id/etRegUsuario2"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginBottom="59dp"
        android:ems="10"
        android:hint="Usuario"
        android:inputType="textPersonName"
        android:paddingLeft="20dp"
        app:layout_constraintBottom_toTopOf="@+id/btRegRegistrarse2"
        app:layout_constraintStart_toStartOf="@+id/etRegEmail2"
        app:layout_constraintTop_toBottomOf="@+id/etRegEmail2" />

    <Button
        android:id="@+id/btRegRegistrarse2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="37dp"
        android:layout_marginBottom="209dp"
        android:onClick="validarclave"
        android:text="registrarse"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/etRegUsuario2"
        app:layout_constraintTop_toBottomOf="@+id/etRegUsuario2" />
</androidx.constraintlayout.widget.ConstraintLayout>